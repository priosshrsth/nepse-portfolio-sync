{
  "name": "my-stocks",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "start": "next start",
    "lint:next": "next lint",
    "lint:css": "NODE_ENV=production stylelint \"**/*.{css,less,styl,scss,sass,tsx}\" --ignore-path .gitignore",
    "format": "NODE_ENV=production prettier --write \"**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "lint": "NODE_ENV=production eslint \"**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore --fix",
    "format-check": "NODE_ENV=production prettier --check \"**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "tsc": "ENV=production node ./node_modules/typescript/bin/tsc",
    "debug": "node --inspect ./node_modules/next/dist/bin/next",
    "postinstall": "prisma generate",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,scss}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@iconify/react": "^4.1.1",
    "clsx": "^2.1.0",
    "@auth/prisma-adapter": "^1.4.0",
    "@prisma/client": "^5.10.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.25.0",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "next": "^14.1.3",
    "next-auth": "^4.24.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.1.3",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  }
}
